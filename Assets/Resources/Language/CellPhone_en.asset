    m_Metadata:
      m_Items: []
  - m_Id: 9562083328000
    m_Localized: "Mom, I think I\u2019ll sleep over here and come home tomorrow."
    m_Metadata:
      m_Items: []
  - m_Id: 9544916041728
    m_Localized: "Yes, it\u2019s fun. I\u2019ve made a lot of friends :)"
